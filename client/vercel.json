{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps",
  "functions": {
    "api/**/*.js": {
      "runtime": "vercel/node@22.x"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/bin/www"
    }
  ]
}
